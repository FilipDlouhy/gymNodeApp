</div>
<div>
    <a href="/BodyBuilderForm">Create new BodyBuilder</a>
    <a href="/Equipment">Create new Equipment</a>
    <a href="/Suplements">Create new Suplements</a>
    <a href="/Exersize">Create new Exersizes</a>
</div>

    </div>




<div class="show-middle">
    <%if(item.category==="Suplement"){%>
    <div class="selectedItem">

       
<h1>Name: <%= item.name%></h1>   
<h1>Invented in: <%= item.invented%></h1>
<h1>Recomended Dose: <%= item.dose%></h1>
<h1>Price $$: <%= item.price%></h1>
<a href="/Edit/<%=item._id%>/<%=item.category%>">   <button>EDIT</button> </a>
<a href="/Delete/<%=item._id%>/<%=item.category%>"><button>DELETE</button></a>
       </div>
       <%}%>

       <%  if(item.category==="Exersize"){%>
        <div class="selectedItem">
    
           
            <h1>Name: <%= item.name%></h1>   
            <h1>Invented in: <%= item.invented%></h1>
        <h1>Recomended sets: <%= item.recomended%></h1>
           <h1> Max weight: <%= item.max%></h1>
          <a href="/Edit/<%=item._id%>/<%=item.category%>">      <button>EDIT</button></a>
<a href="/Delete/<%=item._id%>/<%=item.category%>"><button>DELETE</button></a>
           </div>
           <%}%>
    
           <%  if(item.category==="Equipment"){%>
            <div class="selectedItem">
        
                <h1>Name: <%= item.name%></h1>   
                <h1>Invented in: <%= item.invented%></h1>
            <h1>Recomended sets: <%= item.recomended%></h1>
            <a href="/Edit/<%=item._id%>/<%=item.category%>">       <button>EDIT</button> </a>
            <a href="/Delete/<%=item._id%>/<%=item.category%>"><button>DELETE</button></a>
               </div>
               <%}%>
               <%  if(item.category==="Bro"){%>
                <div class="selectedItem">
                    <h1>Name: <%= item.name%></h1>   
                    <h1>Age: <%= item.age%></h1>
                <h1>Alias: <%= item.nickname%></h1>
                   <h1>Weight: <%= item.weight%></h1>
                   <h1>Max: <%= item.max%></h1>
                <a href="/Edit/<%=item._id%>/<%=item.category%>"><button>EDIT</button></a>   </a></a>
<a href="/Delete/<%=item._id%>/<%=item.category%>"><button>DELETE</button></a>
                   </div>
                   <%}%>
</div>
</div>



    



 </div>

